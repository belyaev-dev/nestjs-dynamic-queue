{
  "name": "nestjs-dynamic-queue",
  "version": "0.3.0",
  "description": "The nestjs-dynamic-queue package is a NestJS module that allows you to create queues at runtime and send messages to them. It provides a wrapper around the BullMQ package, which is a powerful Redis-based queue library. With nestjs-dynamic-queue, you can easily integrate queue functionality into your NestJS application and leverage the features provided by BullMQ, such as job scheduling, retries, and prioritization.",
  "keywords": [
    "nestjs",
    "queue",
    "bullmq"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint  \"lib/**/*.ts\" --fix",
    "changelog": "npx conventional-changelog-cli conventional-changelog -p angular -i ./CHANGELOG.md -s"
  },
  "author": "Yaroslav Tolstoy <t.me/yatolstoy>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yatolstoy/nestjs-dynamic-queue.git"
  },
  "bugs": {
    "url": "https://github.com/yatolstoy/nestjs-dynamic-queue/issues"
  },
  "homepage": "https://github.com/yatolstoy/nestjs-dynamic-queue#readme",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "bullmq": "^3.0.0"
  },
  "devDependencies": {
    "bullmq": "^3.10.0",
    "@nestjs/common": "10.3.7",
    "@types/node": "^16.18.25",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.2.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.1.3"
  },
  "packageManager": "npm@10.8.2"
}